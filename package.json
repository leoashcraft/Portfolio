    "laravel-mix": "^6.0.49",
    "laravel-vite-plugin": "^1.0",
    "postcss": "^8.4.31",
    "rimraf": "^3.0.2",
    "tailwindcss": "^3.1.0",
    "vite": "^5.0",
    "webpack": "^5.72.0",
    "webpack-cli": "^4.9.2"
  }
